{
  "build": {
    "template": [
      "default",
      "templates/quadspinner"
    ],
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**/toc.yml",
          "api/index.md",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "documentation/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "reference/**.md",
          "reference/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "quickstarts/**.md",
          "quickstarts/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "QuadSpinner Gaea",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/quadspinner/Gaea-Docs",
        "branch": "master"
      }
    },
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}