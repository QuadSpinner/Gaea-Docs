{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "template": [
      "default",
      "templates/quadspinner"
    ],
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**/toc.yml",
          "api/index.md",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "documentation/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "reference/**.md",
          "reference/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "quickstarts/**.md",
          "quickstarts/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  },
  "xxxpdf": {
    "content": [
      {
        "files": [
	  "api/**.md",
          "api/**.yml",
          "api-vb/**.yml"
        ],
        "exclude": [
          "**/toc.yml",
          "**/toc.md"
        ]
      },
      {
        "files": [
          "documentation/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "*.md",
          "pdf/*"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site_pdf/**",
          "**/toc.yml",
          "**/toc.md"
        ]
      },
      {
        "files": "pdf/toc.yml"
      }
    ]
  }
}